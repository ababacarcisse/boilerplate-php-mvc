{
    "name": "ababacarcisse/coud-bouletplate",
    "description": "Un framework PHP MVC léger, sécurisé et facile à utiliser",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ababacarcisse",
            "email": "cissedev5@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0.30",
        "phpmailer/phpmailer": "^6.5",
        "firebase/php-jwt": "^6.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Api\\": "app/api/",
            "App\\Lib\\": "app/Lib/",
            "Core\\": "core/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php -r \"copy('config/config.example.php', 'config/config.php');\""
        ]
    }
}
